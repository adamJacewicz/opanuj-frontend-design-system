{
  "name": "@undefined37/design-system",
  "version": "1.1.0",
  "description": "Design system",
  "license": "MIT",
  "keywords": [
    "design-system"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "npm run clean && npm run build:tokens && npm run build:bundle && npm run test",
    "test": "vitest run",
    "build:bundle": "rollup -c",
    "build:tokens": "node build.js",
    "clean": "rimraf dist",
    "release": "npm run build && np"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "np": "^10.0.7",
    "rimraf": "^6.0.1",
    "rollup": "^4.21.2",
    "rollup-plugin-dts": "^6.1.1",
    "style-dictionary": "^3.9.2",
    "typescript": "^5.4.5",
    "vitest": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamJacewicz/opanuj-frontend-design-system.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
